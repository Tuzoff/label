#Bucket policy
{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "DelegateS3Access",
            "Effect": "Allow",
            "Principal": {
                "AWS": [
                    "arn:aws:iam::937402672546:user/daviath",
                    "arn:aws:iam::937402672546:root"
                ]
            },
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::d-pan1",
                "arn:aws:s3:::d-pan1/*"
            ]
        }
    ]
}

#User policy
{
    "Version": "2012-10-17",
    "Statement": {
        "Effect": "Allow",
        "Action": "s3:*",
        "Resource": [
            "arn:aws:s3:::d-pan1",
            "arn:aws:s3:::d-pan1/*"
        ]
    }
}